{
    "compilerOptions": {
      	"esModuleInterop": true,
		"moduleResolution": "Node",
      	"downlevelIteration": true,
        "strict": true, 
        "sourceMap": true,
        "outDir": "dist",
        "skipLibCheck": true
    },
    "include": ["./**/*.ts"],
    "exclude": ["node_modules",  "./node_modules/*", "dist"]
}
